project lpedataelm
       : source-location .
       : build-dir $(BUILD_TOP)
       : requirements
          <include>$(BOOST)
       ;

exe lpedataelm
       : lpedataelm.cpp
         /libraries/temporary//dataelm
         /libraries/temporary//lowlevel
         /libraries/temporary//print
         /libraries/temporary//struct
       : <source>/boost/program_options//boost_program_options
         <source>/root//mcrl2_revision
         <include>.
       ;

install_with_libraries lpedataelm ;

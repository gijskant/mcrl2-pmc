project md5pp
    : source-location
       ../source
    : build-dir $(BUILD_TOP)
    : requirements
       <include>../..
    : usage-requirements
       <include>../..
    ;

obj md5.o : md5.cpp ;
obj md5pp.o
    : md5pp.cpp
    : <use>/site-config//boost_filesystem
    ;

alias md5pp 
    : md5.o
      md5pp.o
    :
    : <dependency>/site-config//boost_filesystem
    ;
